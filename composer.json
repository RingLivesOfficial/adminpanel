{
    "name": "ringlives/adminpanel",
    "description": "Backend controllers and scaffolding for admin panel.",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/RingLivesOfficial/adminpanel/issues",
        "source": "https://github.com/RingLivesOfficial/adminpanel"
    },
    "authors": [
        {
            "name": "Sahidur Rahman",
            "email": "shohidurr49@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Ringlives\\Adminpanel\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Ringlives\\Adminpanel\\AdminpanelServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}